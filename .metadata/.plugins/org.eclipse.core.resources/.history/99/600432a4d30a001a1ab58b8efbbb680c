package com.loan.service;

public class UserLoginImpl implements UserLoginService{

	UserDaoImpl dao = null;
	dao = new UserDaoImpl();
	@Override
	public boolean checkLogin(String username, String password) {
	
			int result = dao.readLogin(username, password);
			if(result ==1)
				return true;
			else
				return false;
		}
		return false;
	}

}
